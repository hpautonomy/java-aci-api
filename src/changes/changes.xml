<document xmlns="http://maven.apache.org/changes/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
    <body>
        <release version="12.0.0" date="2017-04-27" description="Align release version with IDOL">
        </release>
        <release version="6.0.0" date="2017-06-27" description="InputStream parameters">
            <action date="2017-06-27" dev="Alex Scown" type="add">
                Add support for InputStream parameters
            </action>
            <action date="2016-10-24" dev="Will Byrne" type="update">
                Upgrade to Java 8
            </action>
        </release>
        <release version="5.0.1" date="2016-10-24" description="Patch release">
            <action date="2016-10-24" dev="Alex Scown" issue="FLIB-39" type="fix">
                Fix connection starvation bug on HTTP error status codes
            </action>
        </release>
        <release version="5.0.0" date="2015-07-21" description="Initial FOSS release">
            <action dev="Matthew Gordon" type="add">
                Extracted from internal HP Autonomy VCS.
            </action>
            <action dev="Matthew Gordon" type="remove">
                Removed deprecated connectionManagerTimeout field from the HttpClientFactory class.
            </action>
            <action dev="Matthew Gordon" type="remove">
                Removed BTEA encryption support.
            </action>
        </release>
    </body>
</document>
